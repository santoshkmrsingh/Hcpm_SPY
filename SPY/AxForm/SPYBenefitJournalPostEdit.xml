				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        if(element.args().caller() && element.args().caller() is SPYBenefitJrnlPostServiceController)
        {
            benefitJrnlPostServiceController = element.args().caller() as SPYBenefitJrnlPostServiceController;
            benefitJournalQuery = this.initApplQuery();
    
        }        
    
        element.setTitleText();
    
        if(benefitJrnlPostServiceController.getDataContractObject() is SPYBenefitJournalPostContract)
            benefitJournalPostContract = benefitJrnlPostServiceController.getDataContractObject();
    
        element.initializeValue();
    
        benefitJournalQuery.dataSourceTable(tableNum(SPYBenefitJournalTable)).
        addRange(fieldNum(SPYBenefitJournalTable,BenefitJournalCode)).value(benefitJrnlPostServiceController.parmBenefitJournalCode());
        benefitJournalQueryRun = new QueryRun(benefitJournalQuery);
    }

]]></Source>
