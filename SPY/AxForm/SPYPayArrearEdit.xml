						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            boolean     ret = true;
        
            if(!LegalEntity_ReferenceGroup.value())
                ret = checkFailed("@SPY375");
            if(!PayrollPeriod_ReferenceGroup.value())
                ret = checkFailed("@SPY376");
            if(DateEditTransDate.dateValue() == dateNull())
                ret = checkFailed("@SPY377");
        
            if(ret)
                super();
        }

]]></Source>
