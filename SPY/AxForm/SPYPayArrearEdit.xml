[Form]
public class SPYPayArrearEdit extends FormRun implements IDialogable
{
    SPYPayArrearServiceController           payArrearServiceController;
    Query                                   workerQuery;
    QueryRun                                workerQueryRun;
    container                               queryPack;
    SPYPayArrearContract                    payArrearContract;
    boolean                                 calledFromArrearProcess;

}
]]></Source>
			</Method>
