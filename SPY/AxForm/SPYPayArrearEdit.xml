    public FormRun formRun()
    {
        //Required by the SysOperation framework.
        return this;
    }

]]></Source>
			</Method>
			<Method>
				<Name>closedOk</Name>
				<Source><![CDATA[
    public boolean closedOk()
    {
        boolean                     ret;
    
        ret = super();
    
        if(payArrearContract)
        {
            payArrearContract.parmLegalEntity(LegalEntity_ReferenceGroup.value());
            payArrearContract.parmWorkerQuery(queryPack);
            payArrearContract.parmPayrollPeriodLine(PayrollPeriod_ReferenceGroup.value());
            payArrearContract.parmTransDate(DateEditTransDate.dateValue());
            payArrearContract.parmArrearPeriodFrom(ArrearPeriodFrom_ReferenceGroup.value());
            payArrearContract.parmArrearPeriodTo(ArrearPeriodTo_ReferenceGroup.value());
            payArrearContract.parmCheckTrace(checkTrace.value());
            payArrearContract.parmPayrollParameterState(payArrearServiceController.parmPayrollParameterState());
        }
        payArrearServiceController.parmPayrollPeriodLine(PayrollPeriod_ReferenceGroup.value());
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        SPYPayrollParameterState    payrollParameterState;
    
        super();
        if(element.args().caller() && element.args().caller() is SPYPayArrearServiceController)
        {
            payArrearServiceController = element.args().caller() as SPYPayArrearServiceController;
            payrollParameterState = payArrearServiceController.parmPayrollParameterState();
            if(payrollParameterState && payrollParameterState.parmCalledFromArrearProcess())
            {
                calledFromArrearProcess = true;
            }
            else
            {
                workerQuery = this.initApplQuery();
                workerQueryRun = new QueryRun(workerQuery);
            }
        }
    
        element.setEnable();
        if(payArrearServiceController.getDataContractObject() is SPYPayArrearContract)
            payArrearContract = payArrearServiceController.getDataContractObject();
    
        element.initializeValue();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initApplQuery</Name>
				<Source><![CDATA[
    Query initApplQuery()
    {
        return new Query(queryStr(SPYHcmWorker_All));
    }

]]></Source>
			</Method>
			<Method>
				<Name>initializeValue</Name>
				<Source><![CDATA[
    public void initializeValue()
    {
        LegalEntity_ReferenceGroup.value(payArrearContract.parmLegalEntity());
        PayrollPeriod_ReferenceGroup.value(payArrearContract.parmPayrollPeriodLine());
        DateEditTransDate.dateValue(payArrearContract.parmTransDate());
        ArrearPeriodFrom_ReferenceGroup.value(payArrearContract.parmArrearPeriodFrom());
        ArrearPeriodTo_ReferenceGroup.value(payArrearContract.parmArrearPeriodTo());
        queryPack = payArrearContract.parmWorkerQuery();
        checkTrace.value(payArrearContract.parmCheckTrace());
    }

]]></Source>
			</Method>
			<Method>
				<Name>initQuery</Name>
				<Source><![CDATA[
    void  initQuery()
    {
        if(workerQueryRun.prompt())
        {
            queryPack = workerQueryRun.pack();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>runBase</Name>
				<Source><![CDATA[
    SPYPayArrearServiceController runBase()
    {
        //Method must be named runBase() for the integration to the Batch dialog to work.
        return payArrearServiceController;
    }

]]></Source>
			</Method>
			<Method>
				<Name>setEnable</Name>
				<Source><![CDATA[
    public void setEnable()
    {
        Choose.visible(!calledFromArrearProcess);
    }

]]></Source>
