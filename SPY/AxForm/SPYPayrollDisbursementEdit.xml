				<Source><![CDATA[
    public boolean closedOk()
    {
        boolean                     ret;
    
        ret = super();
    
        if(payrollDisbursementContract)
        {
            payrollDisbursementContract.parmLegalEntity(LegalEntityControl.value());
            payrollDisbursementContract.parmPayrollPeriod(PayrollPeriodControl.value());
            payrollDisbursementContract.parmFinalDate(FinalDateControl.dateValue());
            payrollDisbursementContract.parmWorkerQuery(queryPack);
        }
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        if(element.args().caller() && element.args().caller() is SPYPayrollDisbursementServiceController)
        {
            payrollDisbursementServiceController = element.args().caller() as SPYPayrollDisbursementServiceController;
            workerQuery = this.initApplQuery();
            workerQueryRun = new QueryRun(workerQuery);
        }        
    
        if(payrollDisbursementServiceController.getDataContractObject() is SPYPayrollDisbursementContract)
            payrollDisbursementContract = payrollDisbursementServiceController.getDataContractObject();
    
        element.initializeValue();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initApplQuery</Name>
				<Source><![CDATA[
    Query initApplQuery()
    {
        return new Query(queryStr(HcmWorkerLookup));
    }

]]></Source>
			</Method>
			<Method>
				<Name>initializeValue</Name>
				<Source><![CDATA[
    public void initializeValue()
    {
        LegalEntityControl.value(payrollDisbursementContract.parmLegalEntity());
        PayrollPeriodControl.value(payrollDisbursementContract.parmPayrollPeriod());
        FinalDateControl.dateValue(payrollDisbursementContract.parmFinalDate());
        queryPack = payrollDisbursementContract.parmWorkerQuery();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initQuery</Name>
				<Source><![CDATA[
    void  initQuery()
    {
        if(workerQueryRun.prompt())
        {
            queryPack = workerQueryRun.pack();
        }
    }

]]></Source>
