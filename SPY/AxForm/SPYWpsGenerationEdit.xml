						<Source><![CDATA[
        public boolean modified()
        {
            boolean ret;
        
            ret = super();
        
            DateEditFromDate.dateValue(SPYPayrollPeriodLine::findPrevPeriodLine(this.value()).PayrollDate+1);
            DateEditToDate.dateValue(SPYPayrollPeriodLine::find(this.value()).PayrollDate);
        
            return ret;
        }

