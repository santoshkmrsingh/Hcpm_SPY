			<Method>
				<Name>closedOk</Name>
				<Source><![CDATA[
    public boolean closedOk()
    {
        boolean                     ret;
    
        ret = super();
    
        if(wpsGenerationContract)
        {
            wpsGenerationContract.parmLegalEntity(LegalEntity_ReferenceGroup.value());
            wpsGenerationContract.parmPayrollPeriodLine(PayrollPeriod_ReferenceGroup.value());
            wpsGenerationContract.parmWorkerQuery(queryPack);
            wpsGenerationContract.parmFromDate(DateEditFromDate.dateValue());
            wpsGenerationContract.parmToDate(DateEditToDate.dateValue());
            wpsGenerationContract.parmBankAccountId(CompanyBankAccountId.text());
            wpsGenerationContract.parmWorkerSponsor(WorkerSponsor.value());
        }
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        if(element.args().caller() && element.args().caller() is SPYWpsGenerationServiceController)
        {
            wpsGenerationServiceController = element.args().caller() as SPYWpsGenerationServiceController;
            workerQuery = this.initApplQuery();
            workerQueryRun = new QueryRun(workerQuery);
        }        
    
        if(wpsGenerationServiceController.getDataContractObject() is SPYWpsGenerationContract)
            wpsGenerationContract = wpsGenerationServiceController.getDataContractObject();
    
        element.initializeValue();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initApplQuery</Name>
				<Source><![CDATA[
    Query initApplQuery()
    {
        return new Query(queryStr(HcmWorkerDetail));
    }

]]></Source>
			</Method>
			<Method>
				<Name>initializeValue</Name>
				<Source><![CDATA[
    public void initializeValue()
    {
        LegalEntity_ReferenceGroup.value(wpsGenerationContract.parmLegalEntity());
        PayrollPeriod_ReferenceGroup.value(wpsGenerationContract.parmPayrollPeriodLine());
        queryPack = wpsGenerationContract.parmWorkerQuery();
        DateEditFromDate.dateValue(wpsGenerationContract.parmFromDate());
        DateEditToDate.dateValue(wpsGenerationContract.parmToDate());       
        CompanyBankAccountId.text(wpsGenerationContract.parmBankAccountId());
        WorkerSponsor.value(wpsGenerationContract.parmWorkerSponsor());
    }

]]></Source>
			</Method>
			<Method>
				<Name>initQuery</Name>
				<Source><![CDATA[
    void  initQuery()
    {
        if(workerQueryRun.prompt())
        {
            queryPack = workerQueryRun.pack();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>runBase</Name>
				<Source><![CDATA[
    SPYWpsGenerationServiceController runBase()
    {//Method must be named runBase() for the integration to the Batch dialog to work.
        return wpsGenerationServiceController;
    }

]]></Source>
