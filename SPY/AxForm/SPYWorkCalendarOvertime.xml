			<Method>
				<Name>canClose</Name>
				<Source><![CDATA[
    public boolean canClose()
    {
        boolean                         ret;
        SPYWorkCalendarOvertimeSlab     workCalendarOvertimeSlab;
    
        ret = super();
    
        select firstonly workCalendarOvertimeSlab
            where workCalendarOvertimeSlab.WorkCalendarOvertime == SPYWorkCalendarOvertime.RecId &&
                  (workCalendarOvertimeSlab.Remainder ||
                  workCalendarOvertimeSlab.Hours);
        if(!workCalendarOvertimeSlab)
            ret = checkFailed("@SPY1557");
    
        return ret;
    }

]]></Source>
