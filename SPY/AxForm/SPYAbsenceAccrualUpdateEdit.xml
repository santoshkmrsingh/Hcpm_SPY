						<Name>clicked</Name>
						<Source><![CDATA[
        void clicked()
        {
            boolean     ret = true;
        
            if(!LegalEntity_ReferenceGroup.value())
                ret = checkFailed("@SPY375");
            if(!FromPayrollPeriod_ReferenceGroup.value())
                ret = checkFailed("@SPY1550");
            if(!ToPayrollPeriod_ReferenceGroup.value())
                ret = checkFailed("@SPY1551");
            if(SPYPayrollPeriodLine::find(FromPayrollPeriod_ReferenceGroup.value()).PeriodStartDate >
               SPYPayrollPeriodLine::find(ToPayrollPeriod_ReferenceGroup.value()).PeriodStartDate)
                ret = checkFailed("@SPY1553");
            if(ret)
                super();
        }

]]></Source>
