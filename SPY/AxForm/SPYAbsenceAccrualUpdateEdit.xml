				<Source><![CDATA[
    public boolean closedOk()
    {
        boolean                     ret;
    
        ret = super();
    
        if(absenceAccrualUpdateContract)
        {
            absenceAccrualUpdateContract.parmLegalEntity(LegalEntity_ReferenceGroup.value());
            absenceAccrualUpdateContract.parmFromPayrollPeriodLine(FromPayrollPeriod_ReferenceGroup.value());
            absenceAccrualUpdateContract.parmToPayrollPeriodLine(ToPayrollPeriod_ReferenceGroup.value());
            absenceAccrualUpdateContract.parmWorkerQuery(workerQueryPack);
        }
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        if(element.args().caller() && element.args().caller() is SPYAbsenceAccrualUpdateServiceController)
        {
            absenceAccrualUpdateServiceController = element.args().caller() as SPYAbsenceAccrualUpdateServiceController;
            workerQuery = this.initApplQuery();
            workerQueryRun = new QueryRun(workerQuery);
        }        
    
        if(absenceAccrualUpdateServiceController.getDataContractObject() is SPYAbsenceAccrualUpdateContract)
            absenceAccrualUpdateContract = absenceAccrualUpdateServiceController.getDataContractObject();
    
        element.initializeValue();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initApplQuery</Name>
				<Source><![CDATA[
    Query initApplQuery()
    {
        return new Query(queryStr(SPYHcmWorker_All));
    }

]]></Source>
			</Method>
			<Method>
				<Name>initializeValue</Name>
				<Source><![CDATA[
    public void initializeValue()
    {
        LegalEntity_ReferenceGroup.value(absenceAccrualUpdateContract.parmLegalEntity());
        FromPayrollPeriod_ReferenceGroup.value(absenceAccrualUpdateContract.parmFromPayrollPeriodLine());
        ToPayrollPeriod_ReferenceGroup.value(absenceAccrualUpdateContract.parmToPayrollPeriodLine());
        workerQueryPack = absenceAccrualUpdateContract.parmWorkerQuery();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initWorkerQuery</Name>
				<Source><![CDATA[
    void  initWorkerQuery()
    {
        if(workerQueryRun.prompt())
        {
            workerQueryPack = workerQueryRun.pack();
        }
    }

]]></Source>
