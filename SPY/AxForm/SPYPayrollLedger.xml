	<SourceCode>
		<Methods xmlns="">
			<Method>
				<Name>classDeclaration</Name>
				<Source><![CDATA[
[Form]
public class SPYPayrollLedger extends FormRun
{
    boolean                                      EOSRunCaller;
    SPYEOSRunRecId                         EOSRun;
    QueryBuildRange                         EOSRunRange;

    boolean                                      masterProcessCaller;
    HcmWorkerRecId                         worker;
    SPYPayrollPeriodLineRecId            payrollPeriodLine;
    QueryBuildRange                         workerRange;
    QueryBuildRange                         payrollPeriodLineRange;

    SPYPayrollEarningsJournalTable    payrollEarningsJournalTable;
    QueryBuildRange                         payrollEarningsJournalRange;

}
]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        SPYEOSRunTable      EOSRunTable;
        FormRun                  formRun;
        container                 argsCon;
    
        if(element.args().record() is SPYPayrollEarningsJournalTable)
            payrollEarningsJournalTable = element.args().record() as SPYPayrollEarningsJournalTable;
        else if(element.args().caller() && element.args().record() is SPYEOSRunTable)
        {
            EOSRunTable = element.args().record() as SPYEOSRunTable;
            EOSRun = EOSRunTable.RecId;
            EOSRunCaller = true;
        }
        else if(element.args().caller())
        {
            formRun = element.args().caller();
            if(formRun.name() == formStr(SPYMasterProcess) || formRun.name() == formStr(SPYPayArrearProcess))
            {
                masterProcessCaller = true;
                argsCon = str2con(element.args().parm());
                worker = conPeek(argsCon,1);
                payrollPeriodLine = conPeek(argsCon,2);
            }
        }
    
        super();
    }

]]></Source>
			</Method>
		</Methods>
		<DataSources xmlns="">
			<DataSource>
				<Name>SPYPayrollLedger</Name>
				<Methods>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            if(payrollEarningsJournalTable)
                payrollEarningsJournalRange.value(queryValue(payrollEarningsJournalTable.RecId));
            else if(EOSRunCaller)
                EOSRunRange.value(queryValue(EOSRun));
            if(masterProcessCaller)
            {
                workerRange.value(queryValue(worker));
                payrollPeriodLineRange.value(queryValue(payrollPeriodLine));
            }
            super();
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            QueryBuildDataSource    EOSPayrollDataSource;
            QueryBuildDataSource    payrollLedgerDS;
            QueryBuildDataSource    payrollEarningsJournalTransDS;
        
            super();
        
            if(payrollEarningsJournalTable)
            {
                payrollLedgerDS = this.query().dataSourceTable(tableNum(SPYPayrollLedger));
                payrollEarningsJournalTransDS = payrollLedgerDS.addDataSource(tableNum(SPYPayrollEarningsJournalTrans));
                payrollEarningsJournalTransDS.addLink(fieldnum(SPYPayrollLedger,PayrollEarningsJournalTrans),fieldNum(SPYPayrollEarningsJournalTrans,RecId));
                payrollEarningsJournalRange = payrollEarningsJournalTransDS.addRange(fieldNum(SPYPayrollEarningsJournalTrans,PayrollEarningsJournal));
            }
            else if(EOSRunCaller)
            {
                EOSPayrollDataSource = this.query().dataSourceTable(tableNum(SPYPayrollLedger)).addDataSource(tableNum(SPYEOSRunPayroll));
                EOSPayrollDataSource.addLink(fieldNum(SPYPayrollLedger,RecId),fieldNum(SPYEOSRunPayroll,PayrollLedger));
                EOSRunRange = EOSPayrollDataSource.addRange(fieldNum(SPYEOSRunPayroll,EOSRun));
            }
            if(masterProcessCaller)
            {
                workerRange = this.query().dataSourceTable(tableNum(SPYPayrollLedger)).addRange(fieldNum(SPYPayrollLedger,Worker));
                payrollPeriodLineRange = this.query().dataSourceTable(tableNum(SPYPayrollLedger)).addRange(fieldNum(SPYPayrollLedger,PayrollPeriodLine));
            }
        }

]]></Source>
					</Method>
