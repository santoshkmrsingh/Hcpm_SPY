								<Name>jumpRef</Name>
								<Source><![CDATA[
            /// <summary>
            ///
            /// </summary>
            public void jumpRef()
            {
                Args    args = new Args();
                SPYPayrollJournalTable       payrollJournalTable = SPYPayrollJournalLine::find(SPYPayrollLedger.PayrollJournalLine).payrollJournalTable();
                args.record(payrollJournalTable);
                args.lookupRecord(payrollJournalTable);

                //super();
                new MenuFunction(menuitemDisplayStr(SPYPayrollJournal), MenuItemType::Display).run(args);
            }

]]></Source>
