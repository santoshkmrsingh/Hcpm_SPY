				<Methods>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        public void init()
        {
            QueryBuildDataSource    payAdjustmentLedgerDS;
            QueryBuildDataSource    payAdjustmentTransDS;
        
            super();
        
            if(payAdjustmentTable)
            {
                payAdjustmentLedgerDS = this.query().dataSourceTable(tableNum(SPYPayAdjustmentLedger));
                payAdjustmentTransDS = payAdjustmentLedgerDS.addDataSource(tableNum(SPYPayAdjustmentTrans));
                payAdjustmentTransDS.addLink(fieldnum(SPYPayAdjustmentLedger,PayAdjustmentTrans),fieldNum(SPYPayAdjustmentTrans,RecId));
                payAdjustmentRange = payAdjustmentTransDS.addRange(fieldNum(SPYPayAdjustmentTrans,PayAdjustment));
            }
        }

]]></Source>
					</Method>
					<Method>
						<Name>executeQuery</Name>
						<Source><![CDATA[
        public void executeQuery()
        {
            if(payAdjustmentTable)
                payAdjustmentRange.value(queryValue(payAdjustmentTable.RecId));
        
            super();
        }

]]></Source>
