					<Method>
						<Name>getBenefitJournalHistoryMessage</Name>
						<Source><![CDATA[
        /// <summary>
        /// Display method to show expense report history message list on the History form.
        /// </summary>
        display str getBenefitJournalHistoryMessage(WorkflowTrackingCommentTable _workflowTrackingCommentTable)
        {
            return SysWorkflowApprovalWorkitemService::getWorkflowHistoryMessage(_workflowTrackingCommentTable);
        }

]]></Source>
					</Method>
					<Method>
						<Name>getBenefitJournalWorkflowComment</Name>
						<Source><![CDATA[
        /// <summary>
        /// Display method to show the workflow comment on the History form.
        /// </summary>
        display str getBenefitJournalWorkflowComment(WorkflowTrackingCommentTable _workflowTrackingCommentTable)
        {
            return _workflowTrackingCommentTable.Comment ? strFmt("@SYS111012", _workflowTrackingCommentTable.Comment) : '' ;      
        }

]]></Source>
					</Method>
					<Method>
						<Name>init</Name>
						<Source><![CDATA[
        /// <summary>
        /// Add range to the query based on the selected expense report on the list page.
        /// </summary>
        public void init()
        {
            SPYBenefitjournalTable    benefitjournalTable;
            QueryBuildRange qbr;

            if (element.args() && element.args().dataset() == tableNum(SPYBenefitjournalTable))
            {
                benefitjournalTable = element.args().record();
            }
            else
            {
                throw error("@SYS22996");
            }
            
            super();
  
            element.design().caption(strFmt("@SPY:WorkflowHistoryCaption", benefitjournalTable.BenefitJournalCode));
 
            Query query = this.query();
            QueryBuildDataSource qbds = query.dataSourceName(tableId2Name(tablenum(WorkflowTrackingStatusTable)));
            qbr = SysQuery::findOrCreateRange(qbds, fieldNum(WorkflowTrackingStatusTable, ContextRecId));
            qbr.value(queryValue(benefitjournalTable.RecId));

            qbr = SysQuery::findOrCreateRange(qbds, fieldNum(WorkflowTrackingStatusTable, ContextTableId));
            qbr.value(queryValue(tablenum(SPYBenefitjournalTable)));
        }

]]></Source>
					</Method>
				</Methods>
