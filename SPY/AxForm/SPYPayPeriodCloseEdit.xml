			<Method>
				<Name>closedOk</Name>
				<Source><![CDATA[
    public boolean closedOk()
    {
        boolean                     ret;
    
        ret = super();
    
        if(payPeriodCloseContract)
        {
            payPeriodCloseContract.parmLegalEntity(LegalEntity_ReferenceGroup.value());
            payPeriodCloseContract.parmPayrollPeriodLine(PayrollPeriod_ReferenceGroup.value());
            payPeriodCloseContract.parmWorkerQuery(workerQueryPack);
        }
    
        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        super();
        if(element.args().caller() && element.args().caller() is SPYPayPeriodCloseServiceController)
        {
            payPeriodCloseServiceController = element.args().caller() as SPYPayPeriodCloseServiceController;
            workerQuery = this.initApplQuery();
            workerQueryRun = new QueryRun(workerQuery);
        }        
    
        if(payPeriodCloseServiceController.getDataContractObject() is SPYPayPeriodCloseContract)
            payPeriodCloseContract = payPeriodCloseServiceController.getDataContractObject();
    
        element.initializeValue();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initApplQuery</Name>
				<Source><![CDATA[
    Query initApplQuery()
    {
        return new Query(queryStr(SPYHcmWorker_All));
    }

]]></Source>
			</Method>
			<Method>
				<Name>initializeValue</Name>
				<Source><![CDATA[
    public void initializeValue()
    {
        LegalEntity_ReferenceGroup.value(payPeriodCloseContract.parmLegalEntity());
        PayrollPeriod_ReferenceGroup.value(payPeriodCloseContract.parmPayrollPeriodLine());
        workerQueryPack = payPeriodCloseContract.parmWorkerQuery();
    }

]]></Source>
			</Method>
			<Method>
				<Name>initWorkerQuery</Name>
				<Source><![CDATA[
    void  initWorkerQuery()
    {
        if(workerQueryRun.prompt())
        {
            workerQueryPack = workerQueryRun.pack();
        }
    }

]]></Source>
			</Method>
			<Method>
				<Name>runBase</Name>
				<Source><![CDATA[
    SPYPayPeriodCloseServiceController runBase()
    {//Method must be named runBase() for the integration to the Batch dialog to work.
        return payPeriodCloseServiceController;
    }

]]></Source>
