                case enum2Value(AllOpenPosted::All) :
                    this.query().dataSourceTable(tableNum(SPYOvertimeJournalTable)).
                    addRange(fieldnum(SPYOvertimeJournalTable,Posted)).value('0,1');
                    break;
        
                case enum2Value(AllOpenPosted::Open) :
                    this.query().dataSourceTable(tableNum(SPYOvertimeJournalTable)).
                    addRange(fieldnum(SPYOvertimeJournalTable,Posted)).value('0');
                    break;
        
                case enum2Value(AllOpenPosted::Posted) :
                    this.query().dataSourceTable(tableNum(SPYOvertimeJournalTable)).
                    addRange(fieldnum(SPYOvertimeJournalTable,Posted)).value('1');
                    break;
            }
            this.query().dataSourceTable(tableNum(SPYOvertimeJournalTable)).addOrderByField(fieldNum(SPYOvertimeJournalTable,createdDateTime),SortOrder::Descending);
        }

]]></Source>
