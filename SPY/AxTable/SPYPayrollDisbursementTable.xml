		<Methods>
			<Method>
				<Name>canSubmitToWorkflow</Name>
				<Source><![CDATA[
    public boolean canSubmitToWorkflow(str _workflowType = '')
    {
        boolean                         ret;
        SPYPayrollDisbursementLine      payrollDisbursementLine;
    
        ret = super(_workflowType);
    
        if(this.WorkflowStatus == SPYWorkflowStatus::Created && !this.Posted)
        {
            select count(recid) from payrollDisbursementLine
                where payrollDisbursementLine.PayrollDisbursement == this.RecId;
            if(payrollDisbursementLine.RecId > 0)
                ret = true;
        }
    
        return ret;
    }

]]></Source>
			</Method>
