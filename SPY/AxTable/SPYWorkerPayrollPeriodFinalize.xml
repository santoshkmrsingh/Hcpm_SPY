]]></Declaration>
		<Methods>
			<Method>
				<Name>exist</Name>
				<Source><![CDATA[
    static boolean exist(HcmWorkerRecId _worker,LegalEntityRecId _legalEntity,SPYPayrollPeriodLineRecId _payrollPeriodLine)
    {
        return _worker && _legalEntity && _payrollPeriodLine &&
                (select firstonly RecId from SPYWorkerPayrollPeriodFinalize
                                    index hint Worker_AK
                                    where SPYWorkerPayrollPeriodFinalize.Worker == _worker &&
                                          SPYWorkerPayrollPeriodFinalize.LegalEntity == _legalEntity &&
                                          SPYWorkerPayrollPeriodFinalize.PayrollPeriodLine == _payrollPeriodLine).RecId != 0;
    }

]]></Source>
			</Method>
		</Methods>
