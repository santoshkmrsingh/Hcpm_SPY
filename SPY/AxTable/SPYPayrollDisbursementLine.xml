		<Methods>
			<Method>
				<Name>nextLineNum</Name>
				<Source><![CDATA[
    LineNum nextLineNum(SPYPayrollDisbursementRecId _payrollDisbursement)
    {
        SPYPayrollDisbursementLine      payrollDisbursementLine;
        LineNum                         ret;
    
        select maxOf(LineNum) from payrollDisbursementLine
            where payrollDisbursementLine.PayrollDisbursement == _payrollDisbursement;
    
        ret = payrollDisbursementLine.LineNum + 1;
        return ret;
    }

]]></Source>
